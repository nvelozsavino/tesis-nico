clear all;
close all;
clc;
numImag=5;
veces=10;
pasos=50:50:500;
a=[];
m=[];
s=[];

for v=1:(length(pasos))
    nombre=strcat('archivos/sound-',num2str(pasos(v),'%.2f'));
    [at,mt,st]=sitios(nombre,numImag,veces);
    a{v}=at';
    m(v)=mt;
    s(v)=st;
end

errorbar(pasos,m*180/pi,s*180/pi,'kx'); hold on;
p=polyfit(pasos,m*180/pi,1);
y=polyval(p,pasos);
plot(pasos,y);
